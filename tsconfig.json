{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "src"
  ],
  "exclude": ["dist"],
  "compilerOptions": {
    "rootDir": "src",
    "baseUrl": "src",
    "ignoreDeprecations": "6.0",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["*"]
    }
  }
}
